<head>
<link rel="stylesheet" href="/css/consulta.css">
</head>
<h1>Consulta de produtos</h1>
<hr>
<table>
    <thead>
        <th>código</th>
        <th>Nome</th>
        <th>Quantidade</th>
        <th>Preço</th>
    </thead>
    <tbody>
    {{#each valores}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.nome}}</td>
            <td>{{this.qtde}}</td>
            <td>{{this.preco}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>
